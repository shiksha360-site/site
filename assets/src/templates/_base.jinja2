<!doctype html>
<html>
  <head>
    <script src="https://cdn.jsdelivr.net/npm/@msgpack/msgpack@2.7.1"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="prod/base.min.js?v={{uuid_gen()}}"></script>
    <script src="prod/login.min.js?v={{uuid_gen()}}"></script>
    {%- block head -%}
    {%- endblock -%}
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="description" content="Save yourselves the trouble of finding good resources to learn!">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Add Material font (Roboto) and Material icon as needed -->
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
    <link rel="preconnect" href="https://cdn.jsdelivr.net/" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@300;400;700&family=Roboto+Slab:wght@300;400;700&family=Roboto:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap" rel="stylesheet" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Material+Icons" rel="stylesheet" crossorigin>

    <!-- Material CSS -->
    <link href="https://cdn.jsdelivr.net/gh/djibe/material@4.6.0-rc.2/css/material.min.css" rel="stylesheet" crossorigin>

    <link href="https://cdn.jsdelivr.net/gh/djibe/material@4.6.0-rc.2/css/material-plugins.min.css" rel="stylesheet" crossorigin>
    

    <link href="prod/base.min.css?v={{uuid_gen()}}" rel="stylesheet"></link>
    {%- block title -%}
        <title>Shiksha360</title>
    {%- endblock -%}
  </head>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
      <ul class="navbar-nav" id="main-navbar">
        <li class="nav-item active" id="navbar-login-check-item">
          <a class="nav-link" id="navbar-login-check" href="#"></a>
        </li>
      </ul>
    </div>
  </nav>
  <script defer>
    $(document).ready(loginCheck)
  </script>
  <body>
    {%- block header -%}
    <h2 class="title text-center"><a style="color: black !important" href="/">Shiksha360</a></h2>
    <h4 class="title text-center"><strong>Truly Hollistic Learning</strong></h4>
    <!-- Basic Modal -->
    <div class="modal fade" id="base-modal" tabindex="-1" aria-labelledby="base-modal-label">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="base-modal-label"></h5>
            <button class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div id="base-modal-body" class="modal-body">
          </div>
          <div id="base-modal-footer" class="modal-footer">
            <button class="btn btn-primary" data-dismiss="modal" aria-label="Close">Close</button>
          </div>
        </div>
      </div>
    </div>
    {%- endblock -%}
    <span id="mobile-banner"></span>
    {%- block content -%}
      <div id="toc-container">
        <h4 id="toc" class="title"><span id="load-title">Loading...</span></h4>
      </div>
    {%- endblock -%}

    <!-- jQuery, Bootstrap Bundle (includes Popper) and Material -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js" crossorigin defer></script>
    <script src="https://cdn.jsdelivr.net/gh/djibe/material@4.6.0-rc.2/js/material.min.js" crossorigin defer></script>
  </body>
</html> 